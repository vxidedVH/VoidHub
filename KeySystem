-- Scripts:

local function SYUEIPH_fake_script() -- closeButton.LocalScript 
	local script = Instance.new('LocalScript', closeButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(SYUEIPH_fake_script)()
local function FKHLOQ_fake_script() -- loginButton.LocalScript 
	local script = Instance.new('LocalScript', loginButton)

	script.Parent.MouseButton1Click:Connect(function()
		if script.Parent.Parent.username.Text == "VoidedHubA-213" then --replace "test username" with your account name(not your roblox account)
			if script.Parent.Parent.password.Text == "Vxided6582321335234" then --replace "test password" with your password(not your roblox password!!!"
				script.Parent.Parent.Parent.Done.Visible = true --change this line of script to the thing you want it to happend after you logging in 
				script.Parent.MouseButton1Click:Connect(function()
					script.Parent.Parent.Visible = false
				end)
			end
		else
			script.Parent.Text = "Incorrect Username or Password." --that is the text when you type incorrect username or password
			wait(1)
			script.Parent.Text = "Login" --dont change this
		end
	end)
	
end
coroutine.wrap(FKHLOQ_fake_script)()
local function INEAZ_fake_script() -- OpenVoidhubbutton.LocalScript 
	local script = Instance.new('LocalScript', OpenVoidhubbutton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(INEAZ_fake_script)()
